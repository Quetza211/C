#include <stdio.h>
//Multiplicacion por sumas
int main(){
	int num1, num2, i, num3 = 0;
	printf("Escribe dos numeros para la multiplicacion \n");
	printf("Numero 1: ");
	scanf("%d",&num1);
	printf("Numero 2: ");
	scanf("%d",&num2);
	
	for(i=1;i<=num1;i++){
		num3 = num3 + num2;
		printf("%d \n",num3);
	}
	printf("El resultado es %d",num3);
	
	
}
